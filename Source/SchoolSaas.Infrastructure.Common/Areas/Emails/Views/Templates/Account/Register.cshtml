@using ACG.MAEC.EConsulatMA.Core.Application.Identity.DataTransferObjects
@model TemplateViewModel<UserCreateDto>
<table>

    <tbody>
        <tr>
            <td>
                <img height="65px" style="vertical-align:middle"
                     src="data:image/svg+xml;base64,">
                <br />
                <span style="font-size: 35px">
                    @localizer["Hello"] @Model.Data.FirstName @Model.Data.LastName,
                </span>
            </td>
        </tr>
        <tr>
            <td>@localizer["Register_P1"]</td>
        </tr>
        <tr>
            <td>@localizer["Register_P2"] @Model.Data.UserName</td>
        </tr>
        <tr>
            <td>@localizer["Register_P3"] @Model.Data.Password</td>
        </tr>
        <tr>
            <table>
                <tr>
                    <td>
                        <a href="https://econsulatma-fo-dev.azurewebsites.net/"
                           class="link-button accept-link" style="color:white">
                            @localizer["Login"]

                        </a>
                    </td>
                </tr>
                <tr>
                    <td>@localizer["IgnoreThisEmail"]</td>
                </tr>
                <tr>
                    <td>
                        <span>
                            @localizer["Regards"],<br>
                            @localizer["EConsulatMATeam"]
                        </span>
                    </td>
                </tr>
            </table>
        </tr>
    </tbody>
</table>

